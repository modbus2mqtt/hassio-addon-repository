    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.3",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "debug": "^4.3.6",
    "express": "^5.0.1",
    "express-fileupload": "^1.5.1",
    "http-server": "^14.1.1",
    "ignore-walk": "^7.0.0",
    "jest-cli": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "latest": "^0.2.0",
    "modbus-serial": "^8.0.17",
    "mqtt": "^5.9.1",
    "multer": "^1.4.5-lts.1",
    "node-html-parser": "^6.1.13",
