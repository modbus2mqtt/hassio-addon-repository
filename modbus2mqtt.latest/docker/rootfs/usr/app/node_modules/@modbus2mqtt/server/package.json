    "@jest/types": "^29.6.3",
    "@opengovsg/credits-generator": "^1.0.7",
    "@popperjs/core": "^2.11.8",
    "@types/adm-zip": "^0.5.5",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/debug": "^4.1.12",
